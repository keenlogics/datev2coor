<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="bool.type">
    <xs:annotation>
      <xs:documentation>Standardwerte für Boolean</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="collect.type">
    <xs:annotation>
      <xs:documentation>Standardwerte für Sammelelement</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="text256.type">
    <xs:annotation>
      <xs:documentation>Standardtext mit max. Länge 256</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="256"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="text128.type">
    <xs:annotation>
      <xs:documentation>Standardtext mit max. Länge 128</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="128"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="text64.type">
    <xs:annotation>
      <xs:documentation>Standardtext mit max. Länge 64</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="64"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="text48.type">
    <xs:annotation>
      <xs:documentation>Standardtext mit max. Länge 48</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="48"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="text32.type">
    <xs:annotation>
      <xs:documentation>Standardtext mit max. Länge 32</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="32"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="text16.type">
    <xs:annotation>
      <xs:documentation>Standardtext mit max. Länge 16</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="16"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="text12.type">
    <xs:annotation>
      <xs:documentation>Standardtext mit max. Länge 12</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="12"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="text8.type">
    <xs:annotation>
      <xs:documentation>Standardtext mit max. Länge 8</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="8"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="text4.type">
    <xs:annotation>
      <xs:documentation>Standardtext mit max. Länge 4</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="4"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="invoicetype.type">
    <xs:annotation>
      <xs:documentation>Kennungen der Rechnungsarten in COOR</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="8"/>
      <!--<xs:enumeration value="ER"/>
      <xs:enumeration value="AR"/>
      <xs:enumeration value="TS"/>
      <xs:enumeration value="SR"/>
      <xs:enumeration value="RR"/>
      <xs:enumeration value="VZ"/>
      <xs:enumeration value="HRL"/>-->
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="scope.type">
    <xs:annotation>
      <xs:documentation>Kennungen der COOR Bereiche</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="bkm.city"/>
      <xs:enumeration value="bkm.bank"/>
      <xs:enumeration value="bkm.part"/>
      <xs:enumeration value="bkm.order"/>
      <xs:enumeration value="bkm.parish"/>
      <xs:enumeration value="bkm.project"/>
      <xs:enumeration value="bkm.placing"/>
      <xs:enumeration value="bkm.payment"/>
      <xs:enumeration value="bkm.contact"/>
      <xs:enumeration value="bkm.invoice"/>
      <xs:enumeration value="bkm.addition"/>
      <xs:enumeration value="bkm.evident"/>
      <xs:enumeration value="bkm.revision"/>
      <xs:enumeration value="bkm.backup"/>
      <xs:enumeration value="bkm.counterpart"/>
      <xs:enumeration value="ret.invoice"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="valuedata.type">
    <xs:annotation>
      <xs:documentation>Datentypen für die KeyValue Einträge (COOR Ergänzungen)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="string"/>
      <xs:enumeration value="int"/>
      <xs:enumeration value="double"/>
      <xs:enumeration value="date"/>
      <xs:enumeration value="list"/>
      <xs:enumeration value="percent"/>
      <xs:enumeration value="contact"/>
      <xs:enumeration value="counterpart"/>
      <xs:enumeration value="enlisted"/>
      <xs:enumeration value="user"/>
      <xs:enumeration value="workgroup"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="normdata.type">
    <xs:annotation>
      <xs:documentation>Unterstützte AVA Datenträgerformate</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="oenorm"/>
      <xs:enumeration value="gaeb"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="externalkey.type">
    <xs:annotation>
      <xs:documentation>Eindeutiger Schlüssel des Fremdsystems</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="256"/>
      <xs:minLength value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="decimal.type">
    <xs:annotation>
      <xs:documentation>Werte</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:totalDigits value="12"/>
      <xs:fractionDigits value="2"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="decimal8.type">
    <xs:annotation>
      <xs:documentation>Werte</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:totalDigits value="12"/>
      <xs:fractionDigits value="8"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="percent.type">
    <xs:annotation>
      <xs:documentation>Prozent-Werte</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:totalDigits value="18"/>
      <xs:fractionDigits value="6"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="documentviewertype.type">
    <xs:annotation>
      <xs:documentation>Werte für Dokumentenbetrachter</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="STD">
        <xs:annotation>
          <xs:documentation>Windows Standardprogramm</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="IE">
        <xs:annotation>
          <xs:documentation>Internet Explorer</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldrange.type">
    <xs:annotation>
      <xs:documentation>Felder, um einen Eintrag einer Benutzerliste zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="code"/>
      <xs:enumeration value="key"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfielduser.type">
    <xs:annotation>
      <xs:documentation>Felder, um einen Benutzer zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="code"/>
      <xs:enumeration value="extern3"/>
      <xs:enumeration value="login"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldresponse.type">
    <xs:annotation>
      <xs:documentation>Felder, um einen Eintrag zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldcity.type">
    <xs:annotation>
      <xs:documentation>Felder, um einen Ort zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="64"/>
      <xs:enumeration value="zipcode"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfielddocument.type">
    <xs:annotation>
      <xs:documentation>Felder, um ein Dokument zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="name"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldkeyvalue.type">
    <xs:annotation>
      <xs:documentation>Felder, um eine Ergänzung zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="code"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldparish.type">
    <xs:annotation>
      <xs:documentation>Felder, um eine Wirtschaftseinheit zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="code"/>
      <xs:enumeration value="parish"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldcontact.type">
    <xs:annotation>
      <xs:documentation>Felder, um einen Kontakt zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="keyid"/>
      <xs:enumeration value="synonym"/>
      <xs:enumeration value="vatid"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldcounterpart.type">
    <xs:annotation>
      <xs:documentation>Felder, um einen Ansprechpartner zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="lastname"/>
      <xs:enumeration value="mail"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldproject.type">
    <xs:annotation>
      <xs:documentation>Felder, um ein Projekt zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="code"/>
      <xs:enumeration value="number"/>
      <xs:enumeration value="extern3"/>
      <xs:enumeration value="extern3;partstruct"/>
      <xs:enumeration value="part"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldpart.type">
    <xs:annotation>
      <xs:documentation>Felder, um ein Teilprojekte zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="code"/>
      <xs:enumeration value="extern3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldplacing.type">
    <xs:annotation>
      <xs:documentation>Felder, um ein Gewerk zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="code"/>
      <xs:enumeration value="code;partcode1"/>
      <xs:enumeration value="code;groupcode"/>
      <xs:enumeration value="extern3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldrevision.type">
    <xs:annotation>
      <xs:documentation>Felder, um eine Projektänderung zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="number"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldorder.type">
    <xs:annotation>
      <xs:documentation>Felder, um einen Auftrag zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="number"/>
      <xs:enumeration value="numbercol"/>
      <xs:enumeration value="numbercol;parentkey"/>
      <xs:enumeration value="contract"/>
      <xs:enumeration value="extern3"/>
      <xs:enumeration value="placingkey;vendorkey"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldaddition.type">
    <xs:annotation>
      <xs:documentation>Felder, um einen Zusatzauftrag zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="number"/>
      <xs:enumeration value="statuscode"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldevident.type">
    <xs:annotation>
      <xs:documentation>Felder, um einen "Nachtrag beantragt" zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="number"/>
      <xs:enumeration value="numberproject"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldbackup.type">
    <xs:annotation>
      <xs:documentation>Felder, um eine Bürgschaft zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="code"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldavaorder.type">
    <xs:annotation>
      <xs:documentation>Felder, um ein Vertrags-LV zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="number"/>
      <xs:enumeration value="order"/>
      <xs:enumeration value="code"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldavaunit.type">
    <xs:annotation>
      <xs:documentation>Felder, um eine AVA Einheit zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="code"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldinvoice.type">
    <xs:annotation>
      <xs:documentation>Felder, um eine Rechnung zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="number"/>
      <xs:enumeration value="extern1"/>
      <xs:enumeration value="extern2"/>
      <xs:enumeration value="extern3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldpayment.type">
    <xs:annotation>
      <xs:documentation>Felder, um eine Zahlung zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="number"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="matchfieldbank.type">
    <xs:annotation>
      <xs:documentation>Felder, um eine Bank zu identifizieren</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="id"/>
      <xs:enumeration value="bin"/>
      <xs:enumeration value="blz"/>
      <xs:enumeration value="swift"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="coordata">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="bkm.response" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="bkm.user" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="bkm.range" minOccurs="0"/>
        <xs:element ref="bkm.bank" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="bkm.parish" minOccurs="0"/>
        <xs:element ref="bkm.city" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="bkm.contact" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="bkm.counterpart" minOccurs="0"/>
        <xs:element ref="bkm.part" minOccurs="0"/>
        <xs:element ref="bkm.placing" minOccurs="0"/>
        <xs:element ref="bkm.invoice" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="bkm.payment" minOccurs="0"/>
        <xs:element ref="ava.unit" minOccurs="0"/>
        <xs:element ref="bkm.project" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="name" type="text128.type" use="optional"/>
      <xs:attribute name="mandant" type="text64.type" use="optional">
        <xs:annotation>
          <xs:documentation>Die Kennung der COOR Instanz.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="exchangeid" type="xs:int" use="optional">
        <xs:annotation>
          <xs:documentation>IDs des Eintrags in der Tabelle für den Datenaustausch. Wird benötigt, damit der Repsonse mit dem Export zusammengeführt werden kann.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="external" type="text16.type" use="optional" default="STD">
        <xs:annotation>
          <xs:documentation>Kennung des externen Systems. Dadurch wird in COOR festgelegt welche Felder durch das externe System geliefert werden und daher in COOR nicht geändert werden dürfen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="version" type="text16.type" use="optional" default="2.7">
        <xs:annotation>
          <xs:documentation>COOR Versionsnummer um für Erweiterungen gerüstet zu sein. (nur für Notfälle)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="response" type="text256.type" use="optional">
        <xs:annotation>
          <xs:documentation>Voll qualifizierter Dateiname, in dem das Ergebnis des Datentransfers abgelegt wird.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="winuser" type="text128.type" use="optional">
        <xs:annotation>
          <xs:documentation>Windows/Active-Directory Benutzer (in der Regel jener Benutzer, der den Datenexport ausgelöst hat)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="winuserkey" type="externalkey.type" use="optional">
        <xs:annotation>
          <xs:documentation>Der eindeutige, externe Schlüssel des Windows/Active-Directory Benutzer</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="infodate" type="xs:date" use="optional">
        <xs:annotation>
          <xs:documentation>Datum extern (für Freigabeschritte)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="info" type="text256.type" use="optional">
        <xs:annotation>
          <xs:documentation>Zusatzinformation (für Freigabeschritte)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="infouser" type="text128.type" use="optional">
        <xs:annotation>
          <xs:documentation>Benutzer extern (für Freigabeschritte)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>

  <!-- Stammdaten -->
  <xs:element name="bkm.response">
    <xs:complexType>
      <xs:annotation>
        <xs:documentation>Die Antwort von COOR auf einen Import/Export Vorgang</xs:documentation>
      </xs:annotation>
      <xs:attribute name="id" type="xs:int" use="optional">
        <xs:annotation>
          <xs:documentation>Der Primärschlüssel des COOR Elementes</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="status" type="text8.type" use="required">
        <xs:annotation>
          <xs:documentation>Die aktuelle Belegung: 1..in Bearbeitung, 2..OK, 3..Warnung, 4..Fehler. ZUr Zeit werden nur Datensätze mit status in (3,4) geliefert </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="externalkey" type="externalkey.type" use="required">
        <xs:annotation>
          <xs:documentation>Der eindeutige, externe Schlüssel des Eintrages für den die Fehlermeldung/die Warnung erzeugt wurde</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="matchfield" type="matchfieldresponse.type" use="optional">
        <xs:annotation>
          <xs:documentation>Feld, um einen Eintrag zu identifizieren</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="scope" type="scope.type" use="optional">
        <xs:annotation>
          <xs:documentation>Der Name des Knoten im Import-XMLFile. z.Bsp.: bkm.order. In Verbindung mit dem tag externalkey sollte der Ausgangsdatensatz gefunden werden können</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="name" type="text64.type" use="optional">
        <xs:annotation>
          <xs:documentation>Bezeichnung der Ursprungsdaten</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="code" type="text16.type" use="optional">
        <xs:annotation>
          <xs:documentation>Kennung der Ursprungsdaten</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="detail" type="text64.type" use="optional">
        <xs:annotation>
          <xs:documentation>zusätzlich Info aus den Ursprungsdaten</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="message" type="xs:string" use="optional">
        <xs:annotation>
          <xs:documentation>die Fehlermeldung/Warnung</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="extern3" type="text128.type" use="optional">
        <xs:annotation>
          <xs:documentation>ERP Nummer</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="bkm.user">
    <xs:complexType>
      <xs:annotation>
        <xs:documentation>COOR Benutzer</xs:documentation>
      </xs:annotation>
      <xs:attribute name="id" type="xs:int" use="optional">
        <xs:annotation>
          <xs:documentation>Der Primärschlüssel des Benutzers</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="code" type="text128.type" use="optional">
        <xs:annotation>
          <xs:documentation>Synonym des Benutzers (Synonym)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="login" type="text128.type" use="optional">
        <xs:annotation>
          <xs:documentation>Login Benutzers</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="firstname" type="text64.type" use="optional">
        <xs:annotation>
          <xs:documentation>Vorname</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="lastname" type="text64.type" use="optional">
        <xs:annotation>
          <xs:documentation>Nachname</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="externalkey" type="externalkey.type" use="required">
        <xs:annotation>
          <xs:documentation>Der eindeutige, externe Schlüssel des Benutzers</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="matchfield" type="matchfielduser.type" use="optional">
